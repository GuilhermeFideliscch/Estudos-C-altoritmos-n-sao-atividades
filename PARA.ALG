Algoritmo "para"

Var
   numero, soma, cont: inteiro
   media: real



Inicio
      soma <- 0
      para cont de 1 até 10 faca
      escreval("informe um valor: ")
      leia(numero)
      soma <- soma + numero
      fimpara
      media <- soma/cont
      escreval("a media de ",soma,"divido por ", cont," é ",media)



Fimalgoritmo